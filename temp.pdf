					background = "#ffffff";
					position = "absolute";
					cursor = "pointer";
					borderColor = "#1381ce";
					padding = "10px";
					margin = "0px";
					border = "2px solid #d0d0d0";
					clear = "both";
					zIndex = "99";
				};
