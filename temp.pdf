				window.open("/journal/Figure.php?xn=ckd-24-018.xml&id="+fid+"&number=818&p_name="+window.name, "figureView","width=1000,height=1000,scrollbars=yes");
			};
		}
		
		$(".panel").css("margin-bottom","25px");

		$('a[href*="#"]:not([href="#"])').click(function() {
			event.preventDefault();
