<style>
	#overlay { display: none; position: absolute; top: 0px; left: 0px; width: 100%; height: 100%;}
	#overlay #overlay_close { margin:25px auto 0px auto; height: 20px; text-align: right; }
	#overlay #main_image_wrap { margin:25px auto 25px auto; height: 100%; text-align: center; }
</style>
<!-- id="overlay_aa" onmouseleave="fig_hide()" -->
<div id="overlay">
	<div style="width:950px; background-color:#FFFFFF; border: 1px solid #a0a0a0; padding-bottom: 15px; margin: auto;">
	<div id="overlay_close"><img src="/image/b_close.png" alt="Close layer" onclick="closePhotoView(); return false;" style="cursor:pointer;margin-right:25px;" /></div>
	<div id="main_image_wrap" style="background-color:#ffffff;">
	<table style="margin: auto;">
	<tr>
	<!--td onclick="mImage_sliding('prev')" valign="middle" style="cursor: pointer;"><img src="/image/b_prev.png" alt="prev" border="0" /></td-->
	<td valign="middle" style="width: 900px;max-height:510px;"><img id="main_image" src="" width="900" height="510" /></td>
	<!--td onclick="mImage_sliding('next')" valign="middle" style="cursor: pointer;"><img src="/image/b_next.png" alt="next" border="0" /></td-->
	</tr>
		<tr>
			<td style="width:900px;text-align:left;padding-top:10px;" class="main_image_txt">
			</td>
		</tr>
	</table>
	</div>
	</div>
</div>
<script>
	var thumb_array = new Array();
	var thumb_index = 0;

// 	$(window).scroll(function () {
// 		var y = $(this).scrollTop();
// 		$("#overlay").css("margin-top", y+100);
// 	});
	
	function mImage_sliding(dir) {
		 if(dir == 'next') {
			 thumb_index++;
			 if(thumb_index >= thumb_array.length) {
				 thumb_index--;
				 return false;
			 }
		 } else {
			 thumb_index--;
			 if(thumb_index < 0) {
				 thumb_index++;
				 return false;
			 }
		 }

		//170302__踶_
		// ̹ Ȯ  ̹ ֱ .
		if (thumb_array[thumb_index]!="") { 
			value = thumb_array[thumb_index].replace(/thum_/,''); //Ư 
			changeMainImage(value);
		}else { // ̹  
			changeMainImage(thumb_array[thumb_index]);
		}

		//changeMainImage(thumb_array[thumb_index]);
	}
	
	function openPhotoView(obj) {
		var y = $(this).scrollTop();
		$("#overlay").css("margin-top", y+200);
		$("#overlay").show();
		thumb_array = new Array();
		$(obj).parent().parent().find('img.thumb').each(function (index) {
			thumb_array.push($(this).attr("src"));
			if($(obj).attr("src") == $(this).attr("src")) {
				thumb_index = index;
				var t_img = $(this).attr("src").replace('thum_','');
				changeMainImage(t_img);
			}
		});
	}

	function openSnapshot(obj) {
		var y = $(this).scrollTop();
		$("#overlay").css("margin-top", y+200);
		$("#overlay").show();
		thumb_array = new Array();
		$(obj).parent().parent().find('img.thumb').each(function (index) {
			thumb_array.push($(this).attr("src"));
			if($(obj).attr("src") == $(this).attr("src")) {
				thumb_index = index;
				changeMainImage($(this).attr("src"));
			}
		});


	}

		function fig_hide(){	
			$('#overlay').hide();
			$(".change_txt").remove();
			thumb_array = new Array();
			}


	function closePhotoView() {
		$('#overlay').hide();
		$(".change_txt").remove();
		thumb_array = new Array();
	}

	function changeMainImage(src) {
		$("<img />").attr("src", src).load(function() {
			var ratio = 900 / this.width;
			var width = this.width * ratio;
			var height = this.height * ratio;
			if(height > 510) {
				ratio = 510 / this.height;
				width = this.width * ratio;
				height = this.height * ratio;
			}
			$("#overlay #main_image_wrap #main_image").attr('src', this.src);
			$("#overlay #main_image_wrap #main_image").attr('width', width);
			$("#overlay #main_image_wrap #main_image").attr('height', height);
		});
	}



		$("div.sub_journal .article").each(function (i) {
			var artice_height=$(this).outerHeight();
			var img_div=$(this).children('.img_div').outerHeight();
			
			if(img_div < artice_height){
				if($(".main_contents").hasClass("main_contents")){
					$(this).children('.img_div').outerHeight($(this).outerHeight()+5);
				}else{
					$(this).children('.img_div').outerHeight($(this).outerHeight()+35);
				}							
			}
		});
</script>

